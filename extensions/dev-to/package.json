{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "dev-to",
  "title": "DEV Community",
  "description": "List of your dev.to articles",
  "icon": "dev-to.png",
  "author": "lin",
  "categories": [
    "Documentation",
    "Media",
    "Web"
  ],
  "license": "MIT",
  "preferences": [
    {
      "name": "access-token",
      "type": "password",
      "required": true,
      "title": "Personal access token",
      "description": "DEV communit personal access token."
    }
  ],
  "commands": [
    {
      "name": "search-article",
      "title": "Search Article",
      "description": "List dev.to articles",
      "mode": "view"
    },
    {
      "name": "create-article",
      "title": "Create Article",
      "description": "Create dev.to article",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.38.3",
    "node-fetch": "^3.2.10"
  },
  "devDependencies": {
    "@types/node": "^18.7.4",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "^18.0.17",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
